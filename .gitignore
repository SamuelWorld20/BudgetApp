# Flutter-specific ignore rules
.flutter-plugins
.flutter-plugins-dependencies
.packages
.dart_tool/
.idea/
.vscode/

# Build artifacts
build/
.dart_tool/

# Generated files
*.dart_tool
*.iml
*.iws
*.ipr
*.bank

# Android-specific ignore rules
android/app/src/main/assets/flutter_assets/
android/app/build/
android/.gradle/
android/local.properties
android/key.properties
android/keystores/
android/release.keystore
android/debug.keystore

# iOS-specific ignore rules
ios/Flutter/.last_build_id
ios/.symlinks/
ios/Flutter/Generated.xcconfig
ios/Flutter/App.framework/
ios/Flutter/engine/
ios/Pods/
ios/.ios-tags
ios/.flutter-plugins
ios/.flutter-plugins-dependencies
